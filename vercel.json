{
    "version": 2,
    "public": true,
    "builds": [
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "styles.css", "use": "@vercel/static" },
      { "src": "dashboard.js", "use": "@vercel/static" },
      { "src": "assets/**", "use": "@vercel/static" },
      { "src": "public/**", "use": "@vercel/static" },
      { "src": "api/fb_env.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/fb_env.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  